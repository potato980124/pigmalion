<main class="calendar w1100">
    <!-- 달력 위 목표달성률 -->
    <div class="processbox_wrap">
        <div class="t_sucsess">목표달성!!</div>
        <ul class="processbox_bg">
            <li class="cWeight_pig">
                <p class="cWeight"><%= cWeight %><strong>kg</strong></p>
                <div class="cWeight_img"><img src="../img/pig.gif" alt=""></div>
            </li>
            <li class="tWeight_thropy">
                <p class="tWeight"><%= tWeight %><strong>kg</strong></p>
                <div class="tWeight_img"><img src="../img/award.gif" alt=""></div>
            </li>
        </ul>
    </div>
    <!-- 등록정보,달력 -->
    <div class="calendarbox_wrap">
        <!-- 등록정보 -->
        <div class="c_left_regis_info">
            <div class="c_regis_info_tit">
                <h2>2023년 1월 3일 화요일</h2>
                <h1>칼로리 캘린더</h1>
            </div>
            <form action="/apiInfo" method="post">
                <ul class="c_regis_info">
                    <li>
                        <div class="c_regis_info_search">
                            <h3>아침식사</h3>
                            <div class="c_regis_input_wrap">
                                <input class="m_eat" type="text"><button type="button" onclick="foodSearch()"><img src="img/search.png" alt=""></button>
                            </div>
                        </div>
                        <div class="c_regis_input_result">
                            <div class="c_regis_check_wrap m_check">
                            </div>
                        </div>
                    </li>
                    <li>
                        <div class="c_regis_info_search">
                            <h3>점심식사</h3>
                            <div class="c_regis_input_wrap">
                                <input type="text"><button><img src="img/search.png" alt=""></button>
                            </div>
                        </div>
                        <div class="c_regis_input_result">
                        </div>
                    </li>
                    <li>
                        <div class="c_regis_info_search">
                            <h3>저녁식사</h3>
                            <div class="c_regis_input_wrap">
                                <input type="text"><button><img src="img/search.png" alt=""></button>
                            </div>
                        </div>
                        <div class="c_regis_input_result">
                            <div class="c_regis_check_wrap">
                            </div>
                        </div>
                    </li>
                    <li>
                        <div class="c_regis_info_weight">
                            <h3>현재 몸무게</h3>
                            <div class="c_regis_input_wrap">
                                <input type="text"><strong>kg</strong>
                            </div>
                        </div>
                    </li>
                </ul>
            </form>
        </div>
        <!-- 달력 -->
        <div class="right_calendar">
            <table id="calendar_table">
                <colgroup>
                    <col style="width: 14.285%">
                    <col style="width: 14.285%">
                    <col style="width: 14.285%">
                    <col style="width: 14.285%">
                    <col style="width: 14.285%">
                    <col style="width: 14.285%">
                    <col style="width: 14.285%">
                </colgroup>
                <tr id="c_mainBar">
                    <td id="preventMonth" colspan="1"><span id="preventMonth_Button" onclick="prevMonth()">
                            <</span> </td> <td id="yearMonth" colspan="5">
                    </td>
                    <td id="nextMonth" colspan="1"><span id="nextMonth_Button" onclick="nextMonth()">></span></td>
                </tr>
                <tr id="day">
                    <td>Sun</td>
                    <td>Mon</td>
                    <td>Tue</td>
                    <td>Wed</td>
                    <td>Thu</td>
                    <td>Fri</td>
                    <td>Sat</td>
                </tr>
            </table>
        </div>
    </div>
</main>
<script src="js/calendar.js"></script>